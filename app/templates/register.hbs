<div class="col-md-5 col-md-offset-3">
    <div class="form-group">
        <label>Company Name</label>
      {{input class="form-control" value=model.companyName}}
    </div>
    <div class="form-group">
        <label>First Name</label>
      {{input class="form-control" value=model.firstName}}
    </div>
    <div class="form-group">
        <label>Last Name</label>
      {{input class="form-control" value=model.lastName}}
    </div>
    <div class="form-group">
        <label>Email</label>
      {{input type="email" class="form-control" value=model.email}}
    </div>
    <div class="form-group">
        <label>Phone</label>
      {{input class="form-control" value=model.phone}}
    </div>
    <div class="form-group">
        <label>Address</label>
      {{input class="form-control" value=model.address}}
    </div>
    <div class="form-group">
        <label>Password</label>
      {{input class="form-control" value=model.password}}
    </div>
    <div class="form-group">
        <button {{action "register"}} class="btn btn-primary btn-block">Register</button>
    </div>
</div>