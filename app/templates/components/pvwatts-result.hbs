<table class="table table-striped table-condensed">
    <thead>
    <tr>
        <th>Month</th>
        <th>
            AC Energy <br/>
            (kWh)
        </th>

    </tr>
    </thead>

    <tbody>
      {{#each data in result}}
        <tr>
            <td>{{data.month}}</td>
            <td>{{data.ac}}</td>
        </tr>
      {{/each}}
      <tr class="success">
          <td><strong>Annual</strong></td>
          <td>{{acAnnual}}</td>
      </tr>
    </tbody>
</table>